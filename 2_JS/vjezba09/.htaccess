
RewriteEngine on
RewriteCond %{HTTPS} off
RewriteCond %{HTTP:X-Forwarded-SSL} !on
RewriteCond %{HTTP_HOST} ^origo\.hr$ [OR]
RewriteCond %{HTTP_HOST} ^www\.origo\.hr$
RewriteRule ^(.*)$ "https\:\/\/www\.origo\.hr\/$1" [R=301,L]
<FilesMatch "\.js$">
    Header set Content-Type "application/javascript; charset=UTF-8"
</FilesMatch>
